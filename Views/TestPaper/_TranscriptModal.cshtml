<div class="modal fade" id="transcript_modal" tabindex="-1" role="dialog" aria-labelledby="transcript_modalTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="transcript_modalTitle">Transcripts</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="transcript-content"></div>
            <div class="modal-body" id="transcript-content-loading">
